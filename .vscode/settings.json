{
    "markdownlint.config": {
        "MD013": false,
        "MD024": false,
        // H1 Header contain both yaml and body
        "MD025": false,
        "MD026": false,
        "MD029": false,
        "MD033": false,
        "MD034": false,
        // image caption
        "MD045": false,
