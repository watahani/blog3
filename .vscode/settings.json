        "Simons",
        "Writeback",
        "azureemail",
        "certauth",
        "certutil",
        "contoso",
        "dsregcmd",
        "hotmail",
        "kmsi",
        "linkedin",
        "netsh",
        "noreply",
        "onmicrosoft",
        "proxyservice",
        "psso",
        "setreg",
        "signin",
